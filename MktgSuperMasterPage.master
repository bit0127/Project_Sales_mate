                $j("#btnSave").click(function () {

                    var itemCode = $j("#txtItemCode").val();
                    var itemName = $j("#txtItemName").val();
                    var itemPrice = $j("#txtItemPrice").val();
                    var groupId = $j("#ddlGroupName").val();
