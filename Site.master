                                var studentName = studentInfo[i + 1];
                                var picLocation = studentInfo[i + 2];
                                row = row + "<tr><td>'" + studentId + "'</td><td>" + studentName + "</td><td><img width='50' height='55' title='' alt='' src=" + picLocation + "></td><tr>";
