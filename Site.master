                                window.location = "operation.aspx";
                            }
                            else if (msg == "Student") {
                                window.location = "study.aspx";
                            }
                            else {
                                alert(msg);
                                $("#txtUserId").val('');
                                $("#txtPassword").val('')
                            }
                        },
                        failure: function (response) {
                            alert(response.d);
                        }
                    });


                });
            });

