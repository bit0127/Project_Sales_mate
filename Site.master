                        type: "POST",
                        url: "home.aspx/CheckUserData",
                        data: "{uid:'" + uid + "',pwd:'" + pwd + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        async: false,
                        success: function (response) {
                            var msg = response.d;
