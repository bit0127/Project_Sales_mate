                            var htm = tbl + thead + "<tbody>" + row + "</tbody></table>";
                            $("#middle").html("");
                            $("#middle").html(htm);
                            var table = $('#example').DataTable({
                                lengthChange: true,
                                buttons: true
                            });

                            table.buttons().container().insertBefore('#example_filter');
                        },
                        failure: function (response) {
                            alert(response.d);
                        }
                    });
                });
            });
        });
        
        
